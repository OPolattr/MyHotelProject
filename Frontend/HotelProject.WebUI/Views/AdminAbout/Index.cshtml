@model List<ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık 1</th>
        <th>Başlık 2</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutID</th>
            <td>@item.AboutUsTitle1</td>
            <td>@item.AboutUsTitle2</td>
            <td> <a href="/AdminAbout/UpdateAbout/@item.AboutID" class="btn btn-success">Güncelle</a> </td>
        </tr>
    }
</table>